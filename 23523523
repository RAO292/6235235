A = ["O"]
B = ["B"]
E = ['1', '2', '3', '4', '5', '6', '7', '8', '9']
def AC():
    print(E[0]+"|"+E[1]+"|"+E[2])
    print("------")
    print(E[3]+"|"+E[4]+"|"+E[5])
    print("------")
    print(E[6]+"|"+E[7]+"|"+E[8])

AC()
for i in range(1,6):
    C = input()
    E[C] = A
    AC()
    C = input()
    E[C] = B
    AC()
